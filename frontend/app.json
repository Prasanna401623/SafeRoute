{
  "expo": {
    "name": "SafeRoute",
    "slug": "saferoute",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.saferoute.app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.saferoute.app",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.969890596290-n832rrc166o71mk1n2k70jaibh018nv1"
        },
        "googleMapsApiKey": "AIzaSyCKmdgIVLPtPUVDnZFsNx9OJsIergdQsOsY"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.saferoute.app",
      "config": {
        "googleMaps": {
          "apiKey": "PASTE_YOUR_API_KEY_HERE"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow SafeRoute to use your location to find safe routes and provide safety scores."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
